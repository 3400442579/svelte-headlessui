{
  "name": "svelte-headlessui",
  "description": "HeadlessUI components for Svelte",
  "version": "0.0.1-alpha.0",
  "type": "module",
  "keywords": [
    "svelte",
    "component",
    "headless",
    "ui",
    "tailwindcss",
    "tailwindui",
    "renderless"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/captaincodeman/svelte-headlessui.git"
  },
  "author": {
    "name": "Simon Green",
    "email": "simon@captaincodeman.com",
    "url": "https://www.captaincodeman.com/"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "test": "vitest run src",
    "test:watch": "vitest src",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "sourcemaps": "source-map-explorer .svelte-kit/output/client/_app/immutable/pages/*.js .svelte-kit/output/client/_app/immutable/chunks/*.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@tailwindcss/forms": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.1.3",
    "@types/testing-library__jest-dom": "^5.14.5",
    "autoprefixer": "^10.4.7",
    "happy-dom": "^6.0.4",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "source-map-explorer": "^2.5.2",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.13",
    "tailwindcss": "^3.1.5",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vitest": "^0.21.1"
  },
  "dependencies": {
    "svelte-transition": "^0.0.5"
  }
}